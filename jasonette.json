{
  "$jason": {
    "head": {
      "title": "Hamilton Meetings"
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://h-meeting.herokuapp.com/#/",
        "action": {"trigger": "navigate"}
      },
      "footer": {
        "tabs": {
          "items": [{
            "image": "https://raw.githubusercontent.com/BounaixVincent/test-jasonette/master/Hamilton%20Meeting.ico",
            "text": "Meetings",
            "href": {
              "url": "https://raw.githubusercontent.com/BounaixVincent/test-jasonette/master/jasonette.json",
              "view": "jason"
            }
          }, {
            "image": "https://raw.githubusercontent.com/BounaixVincent/test-jasonette/master/Hamilton%20Visitor.ico",
            "text": "Visitors",
            "href": {
              "url": "https://hamiltonapps.com/visitor-management-software-hamilton/",
              "view": "web"
            }
          }, {
            "image": "https://github.com/BounaixVincent/test-jasonette/blob/master/Hamilton%20Workplace.ico",
            "text": "Workplace",
            "href": {
              "url": "https://hamiltonapps.com/workplace/",
              "view": "web"
            }
          }, {
            "image": "https://raw.githubusercontent.com/Jasonette/Twitter-UI-example/master/images/home.png",
            "text": "refresh",
            "action": {
              "type": "$network.request",
              "options": {
                "url": "https://raw.githubusercontent.com/BounaixVincent/test-jasonette/master/jasonette.json"
              },
              "success": {
                "type": "$render"
              }
            }
          }]
        }
      }
    }
  }
}